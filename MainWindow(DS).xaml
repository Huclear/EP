<Window x:Class="Practice4.MainWindow_DS_"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practice4"
        mc:Ignorable="d"
        Title="MainWindow_DS_" Height="450" Width="800">
 <Grid>
     <Grid.RowDefinitions>
         <RowDefinition Height="Auto"/>
         <RowDefinition/>
     </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <ComboBox Name="PageSelection" Margin="12" SelectionChanged="onSelectedPageChanged" HorizontalAlignment="Right" VerticalAlignment="Center"/>
        <Button Content="Go onto Entities" Click="onGoToEntities_Click" Grid.Column="1" Margin="12" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Frame Grid.ColumnSpan="2" Grid.Row="1" Name="Pages" NavigationUIVisibility="Hidden" Margin="8" />
 </Grid>
</Window>
